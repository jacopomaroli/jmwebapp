{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}\\index.js"
        },
        {
            "name": "Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceRoot}/src"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Jest Current File",
            "program": "${workspaceFolder}/node_modules/jest/bin/jest",
            "args": [
              "${relativeFile}",
              "--env=jsdom",
              "--runInBand",
              "--no-cache"
            ],
            "env": {
                "NODE_PATH": "src"
            },
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "routing1.steps.js",
            "program": "${workspaceFolder}/node_modules/jest/bin/jest",
            "args": [
              "test/integration/routing1.steps.js",
              "--env=jsdom",
              "--runInBand",
              "--no-cache"
            ],
            "env": {
                "NODE_PATH": "src"
            },
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "routing2.steps.js",
            "program": "${workspaceFolder}/node_modules/jest/bin/jest",
            "args": [
              "test/integration/routing2.steps.js",
              "--env=jsdom",
              "--runInBand",
              "--no-cache"
            ],
            "env": {
                "NODE_PATH": "src"
            },
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "routing3.steps.js",
            "program": "${workspaceFolder}/node_modules/jest/bin/jest",
            "args": [
              "test/integration/routing3.steps.js",
              "--env=jsdom",
              "--runInBand",
              "--no-cache"
            ],
            "env": {
                "NODE_PATH": "src"
            },
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "routing4.steps.js",
            "program": "${workspaceFolder}/node_modules/jest/bin/jest",
            "args": [
              "test/integration/routing4.steps.js",
              "--env=jsdom",
              "--runInBand",
              "--no-cache"
            ],
            "env": {
                "NODE_PATH": "src"
            },
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        },
        {
            "name": "Debug CRA Tests",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/react-scripts",
            "args": [
              "test",
              "--runInBand",
              "--no-cache",
              "--env=jsdom",
              "--setupTestFrameworkScriptFile=raf/polyfill"
            ],
            "cwd": "${workspaceRoot}",
            "protocol": "inspector",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        }
    ]
}